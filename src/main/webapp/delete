            $("a[href$='#finish']").click(function () {
                message = "Benefit Request is  successfully . Once the benefit is approved by the employer you will get a notification";
                bootbox.alert('<p class="text-center">' + message + '</p>');

                setTimeout(
                    function () {
                        window.location.href = $(
                            '#base_url')
                                .val()
                            + 'member';
                    }, 800);

            });
            $("a[href$='#next']").click(function () {

                $("a[href$='#next']").hide().attr("disabled", "disabled");
            });
            $("a[href$='#previous']").click(function () {
                $("a[href$='#next']").show().removeAttr("disabled");

            });


