            function autoHideBenefitReason() {
                $(document).ready(function () {
                    $('#benefitReason').on('click', function () {
                        var reasonForExit;
                        $.ajax({
                            url: $('#base_url').val() + 'admin',
                            type: 'post',
                            data: {
                                ACTION: 'GET_REASON_FOR_EXIT',
                                reasonId: this.value
                            },
                            dataType: 'json',
                            success: function (json) {
                                if (json.success) {
                                    var obj = $.parseJSON(json.data);
                                    console.log(obj);
                                    reasonForExit = obj.reason;
                                    console.log("reason  "+ obj.reason)
                                }
                            }
                        });
                        console.log("reasonForExit  "+ reasonForExit)

                        if (reasonForExit == 'retirement') {
                            $('#modal-retirement').modal('show');
                            $("#modal-resignation").modal('hide');
                            $("#modal-withdrawal").modal('hide');
                            $("#modal-dismissal").modal('hide');
                            $("#modal-emigration").modal('hide');
                            $("#modal-incapacitation").modal('hide');
                            $("#modal-early-retirement-55").modal('hide');
                            $("#modal-early-retirement-50").modal('hide');
                            $("#retirementId").show();
                            $("#retirementNot").show();
                            $("#resignationId").hide();
                            $("#resignationNot").hide();
