            function getBankBranches() {


                $("#bankBranchDiv").show();

                var bankId = document.getElementById('bankName').value;

                $.ajax({
                    url: $('#base_url').val() + 'admin',
                    type: 'post',
                    data: {
                        ACTION: 'GET_BANK_BRANCHES',
                        bankId: bankId
                    },
                    dataType: 'json',
                    success: function (json) {
                        if (json.success) {
                            json = $.parseJSON(json.data);

                            var combo = "<div class=\"col-lg-2\"></div><div class=\"col-lg-8\"><select id=\"bankBranches\" name=\"bankBranches\" class=\"form-control\"><option>--Select Your Bank Branch--</option>";
                            $.each(json, function (key, value) {
                                if (key == 'rows') {
                                    for (var i = 0; i < json.rows.length; i++) {
                                        var row = json.rows[i];


                                        combo = combo + "<option value = " + row['name'] + ">" + row['name'] + "</option>";

                                        array = json.rows;
                                    }

                                    combo = combo + "</select></div>";

                                }
                            });
                            $('#bankBranchDiv').html(combo);
                        } else {
                            stop_wait();
                            bootbox.alert('<p class="text-center">' + json.message + '</p>');
                        }
                    }
                });


            }

            $(document).ready(function () {
                $('#form-save-continue').bootstrapValidator(
                    {
                        message: 'This value is not valid',
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        },
                        excluded: ':disabled',
                        fields: {}
                    })
                    .on(
                        'success.form.bv',
                        function (e) {
                            start_wait();

                            // Prevent form submission
                            e.preventDefault();
                            var form = "form-save-continue";
                            $.ajax({
                                url: $('#base_url').val() + 'benefit-request',
                                type: 'post',

                                data: {
                                    ACTION: 'ADD_MEMBERINFO',
                                    benefitId: $('#benefitId')
                                        .val(),
                                    memberNo: $('#memberNo')
                                        .val(),
                                    memberEmail: $('#memberEmail')
                                        .val(),
                                    type: 'personal_details'

                                },
                                dataType: 'json',
                                success: function (json) {

                                    stop_wait();
                                    console.log(json.success);//
                                    if (json.success) {
                                        var obj = $.parseJSON(json.data);
                                        $("a[href$='#next']").show().removeAttr("disabled");
                                        document.getElementById("benefitId1").value = obj.benefitId;

                                    } else {
                                        bootbox.alert('<p class="text-center">'
                                            + json.message
                                            + '</p>');
                                    }
                                }


                            });
                        });


                $('#form-save-continue1').bootstrapValidator(
                    {
                        message: 'This value is not valid',
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        },
                        excluded: ':disabled',
                        fields: {
                            benefitReason: {
                                validators: {
                                    notEmpty: {
                                        message: 'Sorry,kindly select ground of benefit!'
                                    }
                                }
                            }
                        }
                    })
                    .on(
                        'success.form.bv',
                        function (e) {
                            start_wait();

                            // Prevent form submission
                            e.preventDefault();
                            var form = "form-save-continue1";

                            $.ajax({
                                url: $('#base_url').val() + 'benefit-request',
                                type: 'post',
                                data: {
                                    ACTION: 'ADD_MEMBERINFO',
                                    benefitId: $('#benefitId1')
                                        .val(),
                                    overseasCountry: $('#overseasCountry')
                                        .val(),
                                    overseasAddress: $('#overseasAddress')
                                        .val(),
                                    overseasTelephone: $('#overseasTelephone')
                                        .val(),
                                    overseasFax: $('#overseasFax')
                                        .val(),
                                    overseasEmail: $('#overseasEmail')
                                        .val(),
                                    overseasBank: $('#overseasBank')
                                        .val(),
                                    overseasIbanDetails: $('#overseasIbanDetails')
                                        .val(),
                                    overseasSwiftCode: $('#overseasSwiftCode')
                                        .val(),
                                    benefitReason: $('#benefitReason')
                                        .val(),
                                    earlyRetirementOption: $('#earlyRetirementOption')
                                        .val(),
                                    type: 'ground_benefits'

                                },
                                dataType: 'json',
                                success: function (json) {
                                    stop_wait();
                                    if (json.success) {
                                        var obj = $.parseJSON(json.data);
                                        $("a[href$='#next']").show().removeAttr("disabled");
                                        document.getElementById("benefitId").value = obj.benefitId;
                                        document.getElementById("benefitId1").value = obj.benefitId;
                                        document.getElementById("benefitId2").value = obj.benefitId;


                                    } else {
                                        bootbox.alert('<p class="text-center">'
                                            + json.message
                                            + '</p>');
                                    }
                                }


                            });
                        });


                $('#form-save-continue2').bootstrapValidator(
                    {
                        message: 'This value is not valid',
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        },
                        excluded: ':disabled',
                        fields: {}
                    })
                    .on(
                        'success.form.bv',
                        function (e) {
                            start_wait();

                            // Prevent form submission
                            e.preventDefault();
                            var form = "form-save-continue2";
                            $.ajax({
                                url: $('#base_url').val() + 'benefit-request',
                                type: 'post',

                                data: {
                                    ACTION: 'ADD_MEMBERINFO',

                                    benefitId: $('#benefitId2')
                                        .val(),
                                    totalAmount: $('#totalAmount')
                                        .val(),
                                    amountPercentage: $('#amountPercentage')
                                        .val(),
                                    paymentOption: $('#paymentOption')
                                        .val(),
                                    type: 'payment_options'
                                },
                                dataType: 'json',
                                success: function (json) {
                                    stop_wait();
                                    if (json.success) {
                                        var obj = $.parseJSON(json.data);
                                        $("a[href$='#next']").show().removeAttr("disabled");

                                        document.getElementById("benefitId").value = obj.benefitId;
                                        document.getElementById("benefitId1").value = obj.benefitId;
                                        document.getElementById("benefitId2").value = obj.benefitId;
                                        document.getElementById("benefitId3").value = obj.benefitId;
                                    } else {
                                        bootbox.alert('<p class="text-center">'
                                            + json.message
                                            + '</p>');
                                    }
                                }


                            });
                        });
                $('#form-save-continue3').bootstrapValidator(
                    {
                        message: 'This value is not valid',
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        },
                        excluded: ':disabled',
                        fields: {}
                    })
                    .on(
                        'success.form.bv',
                        function (e) {
                            start_wait();

                            // Prevent form submission
                            e.preventDefault();
                            var form = "form-save-continue3";
                            $.ajax({
                                url: $('#base_url').val() + 'benefit-request',
                                type: 'post',

                                data: {
                                    ACTION: 'ADD_MEMBERINFO',

                                    benefitId: $('#benefitId3')
                                        .val(),
                                    staffPercentagePerVestingRule: $('#staffPercentagePerVestingRule')
                                        .val(),
                                    employeeOutstandingLoans: $('#employeeOutstadingLoans')
                                        .val(),
                                    employeeOutstandingLoansAmount: $('#employeeOutstadingLoansAmount')
                                        .val(),
                                    staffContribution: $('#staffContribution')
                                        .val(),

                                    mortageChooseOption: $('#mortageChooseOption')
                                        .val(),
                                    mortgageAmount: $('#mortgageAmount')
                                        .val(),
                                    type: 'vesting_liabilities'
                                },
                                dataType: 'json',
                                success: function (json) {
                                    stop_wait();
                                    if (json.success) {
                                        $("a[href$='#next']").show().removeAttr("disabled");
                                        var obj = $.parseJSON(json.data);
                                        document.getElementById("benefitId").value = obj.benefitId;
                                        document.getElementById("benefitId1").value = obj.benefitId;
                                        document.getElementById("benefitId2").value = obj.benefitId;
                                        document.getElementById("benefitId3").value = obj.benefitId;
                                        document.getElementById("benefitId4").value = obj.benefitId;
                                    } else {
                                        bootbox.alert('<p class="text-center">'
                                            + json.message
                                            + '</p>');
                                    }
                                }


                            });
                        });


                $('#form-benefit').bootstrapValidator(
                    {
                        message: 'This value is not valid',
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        },
                        excluded: ':disabled',
                        fields: {
                            bankName: {
                                validators: {
                                    notEmpty: {
                                        message: 'Sorry, your Bank Name is required'
                                    }
                                }
                            },

                            bankBranches: {
                                validators: {
                                    notEmpty: {
                                        message: 'Sorry, your Bank Branch is required'
                                    }
                                }
                            },
                            accountNo: {
                                validators: {
                                    notEmpty: {
                                        message: 'Sorry, your Account Number is required'
                                    }
                                }
                            },
                            accountName: {
                                validators: {
                                    notEmpty: {
                                        message: 'Sorry, your Account Name is required'
                                    }
                                }
                            }

                        }
                    })
                    .on(
                        'success.form.bv',
                        function (e) {
                            start_wait();

                            // Prevent form submission
                            e.preventDefault();
                            var form = "form-benefit";
                            $.ajax({
                                url: $('#base_url').val() + 'benefit-request',
                                type: 'post',

                                data: {
                                    ACTION: 'ADD_MEMBERINFO',
                                    benefitId: $('#benefitId4')
                                        .val(),
                                    bankName: $('#bankName')
                                        .val(),
                                    bankBranch: $('#bankBranches')
                                        .val(),
                                    accountNo: $('#accountNo')
                                        .val(),
                                    accountName: $('#accountName')
                                        .val(),
                                    type: 'bank_details',
                                    submit: 'YES'
                                },
                                dataType: 'json',
                                success: function (json) {
                                    stop_wait();
                                    if (json.success) {
                                        $("form#form-benefit")[0].reset();
                                        setTimeout(
                                            $('#benefitRequestDiv').hide()
                                            , 5000);

                                    }

                                    bootbox.alert('<p class="text-center">'
                                        + json.message
                                        + '</p>');


                                }


                            });
                        });
            });

            $(document).ready(function () {

                $('#form-media4').bootstrapValidator({
                    message: 'This value is not valid',
                    feedbackIcons: {
                        valid: 'glyphicon glyphicon-ok',
                        invalid: 'glyphicon glyphicon-remove',
                        validating: 'glyphicon glyphicon-refresh'
                    },
                    fields: {
                        media_file: {

                            extension: 'doc,docx,xls,xlsx,pdf,jpg,jpeg,png,gif',
                            type: 'image/png,image/gif,image/jpg,image/jpeg',
                            maxSize: 100 * 1024 * 1024,   // 100 MB
                            message: 'The selected file is not valid, it should be (png,gif,jpg,jpeg) and 5 MB at maximum.',
                            validators: {
                                notEmpty: {
                                    message: 'Sorry, Kindly upload a file'
                                }
                            }

                        }
                    }
                })
                    .on('success.form.bv', function (e) {

                        start_wait();

                        // Prevent form submission
                        e.preventDefault();

                        var btn = "btn-file4";
                        var form = "form-media4";

                        $('#' + btn).val('Please wait...');

                        var formData = new FormData($(this)[0]);

                        $.ajax({
                            url: $('#base_url').val() + 'admin',
                            type: 'POST',
                            data: formData,
                            dataType: 'json',
                            async: false,
                            success: function (json) {
                                stop_wait();
                                if (json.success) {
                                    $('#' + form)[0].reset();
                                    $('#labourForm').hide();

                                }
                                bootbox.alert(json.message);
                            },
                            cache: false,
                            contentType: false,
                            processData: false
                        });
                    });
            });
