            function printElem(divId, modalId) {
                var content = document.getElementById(divId).innerHTML;
                var mywindow = window.open('', 'Print', 'height=600,width=800');

                mywindow.document.write('<html><head><title>Print</title>');
                mywindow.document.write('</head><body >');
                mywindow.document.write(content);
                mywindow.document.write('</body></html>');

                mywindow.document.close();
                mywindow.focus();
                mywindow.print();
                mywindow.close();
                $('#' + modalId).modal('hide');

                return true;

            }

            function autoHideOutstandingStaffLoanOnly() {
                $(document).ready(function () {
                    $('#employeeOutstadingLoans').on('click', function () {
                        if (this.value == 'yes') {
                            $("#outstandingStaffLoanOnly").show()
                                .bootstrapValidator(
                                    {
                                        message: 'This value is not valid',
                                        feedbackIcons: {
                                            valid: 'glyphicon glyphicon-ok',
                                            invalid: 'glyphicon glyphicon-remove',
                                            validating: 'glyphicon glyphicon-refresh'
                                        },
                                        excluded: ':disabled',
                                        fields: {
                                            employeeOutstadingLoansAmount: {
                                                validators: {
                                                    notEmpty: {
                                                        message: 'Please provide outstanding loan amount.'
                                                    }
                                                }
                                            }

                                        }
                                    })
                        } else if (this.value == 'no' || this.value == '') {
                            $("#outstandingStaffLoanOnly").hide();
                        }
                    });
                });
            }

            function autoHidefullorPartialEmployerContributionOnly() {
                $(document).ready(function () {
                    $('#staffContribution').on('click', function () {
                        if (this.value == 'yes')
                            //.....................^.......
                        {
                            $("#fullorPartialEmployerContributionOnly").show()
                                .bootstrapValidator(
                                    {
                                        message: 'This value is not valid',
                                        feedbackIcons: {
                                            valid: 'glyphicon glyphicon-ok',
                                            invalid: 'glyphicon glyphicon-remove',
                                            validating: 'glyphicon glyphicon-refresh'
                                        },
                                        excluded: ':disabled',
                                        fields: {
                                            staffPercentagePerVestingRule: {
                                                validators: {
                                                    notEmpty: {
                                                        message: 'Please percentage(%) per vesting rule.'
                                                    }

                                                }
                                            }
                                        }
                                    })
                        } else if (this.value == 'no') {
                            $("#fullorPartialEmployerContributionOnly").hide();
                        } else if (this.value == '') {
                            $("#fullorPartialEmployerContributionOnly").hide();
                        }

                    });
                });
            }

            function mortageOption() {
                $(document).ready(function () {
                    $('#mortageChooseOption').on('click', function () {
                        if (this.value == 'yes')
                            //.....................^.......
                        {
                            $("#mortgageAmountForm").show()
                                .bootstrapValidator(
                                    {
                                        message: 'This value is not valid',
                                        feedbackIcons: {
                                            valid: 'glyphicon glyphicon-ok',
                                            invalid: 'glyphicon glyphicon-remove',
                                            validating: 'glyphicon glyphicon-refresh'
                                        },
                                        excluded: ':disabled',
                                        fields: {
                                            mortgageAmount: {
                                                validators: {
                                                    notEmpty: {
                                                        message: 'Please Enter Amount'
                                                    }

                                                }
                                            }
                                        }
                                    })
                        } else if (this.value == 'no') {
                            $("#mortgageAmountForm").hide();
                        } else if (this.value == '') {
                            $("#mortgageAmountForm").hide();
                        }

                    });
                });
            }

            function autoHidePaymentOption() {
                $(document).ready(function () {
                    $('#paymentOption').on('click', function () {

                        if (this.value == 'totalAmount') {
                            $("#amountPercentageOnly").hide();
                            $("#totalAmountOnly").show()
                                .bootstrapValidator(
                                    {
                                        message: 'This value is not valid',
                                        feedbackIcons: {
                                            valid: 'glyphicon glyphicon-ok',
                                            invalid: 'glyphicon glyphicon-remove',
                                            validating: 'glyphicon glyphicon-refresh'
                                        },
                                        excluded: ':disabled',
                                        fields: {}
                                    })
                        } else if (this.value == 'amountPercentage') {
                            $("#totalAmountOnly").hide();
                            $("#amountPercentageOnly").show()
                                .bootstrapValidator(
                                    {
                                        message: 'This value is not valid',
                                        feedbackIcons: {
                                            valid: 'glyphicon glyphicon-ok',
                                            invalid: 'glyphicon glyphicon-remove',
                                            validating: 'glyphicon glyphicon-refresh'
                                        },
                                        excluded: ':disabled',
                                        fields: {}
                                    })
                        } else if (this.value == '') {
                            $("#totalAmountOnly").hide();
                            $("#amountPercentageOnly").hide();
                        }
                    });
                });
            }


